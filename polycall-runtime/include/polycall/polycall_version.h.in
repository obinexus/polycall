/**
 * @file polycall_version.h
 * @brief Version information for LibPolyCall
 * @author Nnamdi Okpala (OBINexusComputing)
 *
 * This header defines version information constants for the LibPolyCall library.
 * It is automatically generated from polycall_version.h.in during the build process.
 */

 #ifndef POLYCALL_VERSION_H
 #define POLYCALL_VERSION_H
 
 #ifdef __cplusplus
 extern "C" {
 #endif
 
 /**
  * @brief Major version number
  */
 #define POLYCALL_VERSION_MAJOR @POLYCALL_VERSION_MAJOR@
 
 /**
  * @brief Minor version number
  */
 #define POLYCALL_VERSION_MINOR @POLYCALL_VERSION_MINOR@
 
 /**
  * @brief Patch version number
  */
 #define POLYCALL_VERSION_PATCH @POLYCALL_VERSION_PATCH@
 
 /**
  * @brief Full version string
  */
 #define POLYCALL_VERSION_STRING "@POLYCALL_VERSION@"
 
 #ifdef __cplusplus
 }
 #endif
 
 #endif /* POLYCALL_VERSION_H */
{
  "project_name": "books-catalog",
  "description": "Library management system with inventory and lending tracking",
  "port": 5003,
  "database": "books_catalog.db",
  "endpoints": [
    "/books",
    "/authors",
    "/categories",
    "/lending",
    "/inventory"
  ],
  "features": [
    "inventory_management",
    "lending_tracking",
    "search_optimization"
  ],
  "tech_stack": [
    "FastAPI",
    "SQLite",
    "Pydantic",
    "LibPolyCall"
  ],
  "pypolycall_integration": {
    "enabled": true,
    "binding_type": "python-polycall",
    "port_mapping": "3001:444"
  },
  "development": {
    "debug": true,
    "auto_reload": true,
    "log_level": "info"
  }
}